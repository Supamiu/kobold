{
	"name": "@kobold/cli",
	"version": "0.0.1",
	"description": "Kobold CLI",
	"author": "ackwell",
	"license": "MIT",
	"scripts": {
		"start": "yarn clean && babel src --out-dir dist --extensions .ts --watch",
		"build": "yarn clean && yarn compile && yarn declare",
		"clean": "rimraf dist",
		"compile": "babel src --out-dir dist --extensions .ts",
		"declare": "tsc --emitDeclarationOnly",
		"lint": "eslint src --ext .ts",
		"kobold": "node dist"
	},
	"bin": {
		"kobold": "dist/index.js"
	},
	"dependencies": {
		"@babel/runtime-corejs3": "^7.9.2",
		"binary-parser": "^1.6.1",
		"fast-glob": "^3.2.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-typescript": "^7.9.0",
		"@types/binary-parser": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^2.28.0",
		"@typescript-eslint/parser": "^2.28.0",
		"eslint": "^6.8.0",
		"eslint-plugin-prettier": "^3.1.3",
		"prettier": "^2.0.4",
		"rimraf": "^3.0.2",
		"typescript": "^3.8.3",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11"
	}
}
